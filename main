loadstring(game:HttpGet("https://raw.githubusercontent.com/VoidMasterX/strawhook/main/script.lua", true))()

local TeleportService = game:GetService("TeleportService")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

game.Players.PlayerRemoving:Connect(function(plr)
	if plr == game.Players.LocalPlayer then
		local Rejoin = coroutine.create(function()
			local Success, ErrorMessage = pcall(function()
				TeleportService:Teleport(game.PlaceId, LocalPlayer)
				syn.queue_on_teleport(loadstring(game:HttpGet("https://raw.githubusercontent.com/Astro-beep/strawhook/main/main", true))())
			end)

			if ErrorMessage and not Success then
				warn(ErrorMessage)
			end
		end)

		coroutine.resume(Rejoin)
	end
end)
